{
	"name": "@roblox-ts/rojo-resolver",
	"version": "1.1.0",
	"description": "RojoResolver for roblox-ts",
	"keywords": ["rbxts"],
	"repository": {
		"type": "git",
		"url": "https://github.com/roblox-ts/rojo-resolver.git"
	},
	"license": "MIT",
	"author": "osyrisrblx (https://github.com/osyrisrblx)",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"import": "./dist/index.mjs"
		}
	},
	"main": "dist/index.mjs",
	"types": "dist/index.d.mts",
	"files": ["dist", "CHANGELOG.md", "rojo-schema.json", "!**/*.js.map", "!**/*.tsbuildinfo"],
	"scripts": {
		"build": "tsdown",
		"prepublishOnly": "pnpm build",
		"test": "vitest",
		"test:coverage": "vitest --coverage",
		"test:watch": "vitest watch"
	},
	"dependencies": {
		"ajv": "catalog:cli"
	},
	"devDependencies": {
		"@types/fs-extra": "catalog:types",
		"@types/node": "catalog:types",
		"@vitest/coverage-v8": "catalog:test",
		"ts-patch": "catalog:tsc",
		"tsdown": "catalog:build",
		"typescript": "catalog:tsc",
		"typescript-transform-paths": "catalog:tsc",
		"vitest": "catalog:test"
	},
	"engines": {
		"node": ">=22.16.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"nx": {
		"tags": ["scope:tooling", "type:plugin"],
		"projectType": "library",
		"targets": {
			"lint": {
				"command": "eslint",
				"hasTypeAwareRules": true
			},
			"test": {
				"executor": "@nx/jest:jest",
				"options": {
					"jestConfig": "tools/nx/jest.config.cjs",
					"passWithNoTests": true
				}
			}
		}
	},
	"tsdown": {
		"clean": true,
		"dts": {
			"build": true
		},
		"entry": ["src/index.ts"],
		"external": ["typescript"],
		"fixedExtension": true,
		"format": ["esm"],
		"publint": true,
		"shims": true,
		"unused": {
			"level": "warning"
		}
	}
}
